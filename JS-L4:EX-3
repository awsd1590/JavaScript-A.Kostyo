// Отримуємо введені дані від користувача за допомогою prompt()
const userInput = prompt("Введіть дані:");

// Визначаємо тип введених даних
const dataType = typeof userInput;

// Виводимо результат
if (dataType === "number") {
  console.log("Ви ввели число.");
} else if (dataType === "string") {
  console.log("Ви ввели рядок.");
} else {
  console.log("Ви ввели дані іншого типу.");
}
